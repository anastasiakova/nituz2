<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="304.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddCategoryController">
   <children>
      <TextField fx:id="categoryTextField" layoutX="137.0" layoutY="83.0" prefHeight="25.0" prefWidth="240.0" />
      <Label layoutX="132.0" layoutY="14.0" text="Add Category">
         <font>
            <Font name="Calibri Bold" size="44.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="76.0" prefHeight="38.0" prefWidth="123.0" text="Insert Category name" />
      <Button fx:id="addButton" layoutX="386.0" layoutY="81.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="25.0" prefWidth="78.0" text="Add">
         <font>
            <Font name="Calibri" size="15.0" />
         </font></Button>
      <ListView fx:id="cetgoriesTable" layoutX="19.0" layoutY="141.0" prefHeight="117.0" prefWidth="469.0" />
      <Label layoutX="18.0" layoutY="114.0" text="Categories:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="228.0" layoutY="265.0" mnemonicParsing="false" onAction="#closeWind" text="back">
         <font>
            <Font size="15.0" />
         </font></Button>
